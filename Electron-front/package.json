{
  "name": "electron-trainer-clean",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "npm run build:preload && electron-vite dev",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build": "npm run build:preload && electron-vite build",
    "preview": "electron-vite preview",
    "typecheck": "tsc -b --pretty",
    "prepare:monaco": "node src/scripts/copy-monaco.mjs",
    "postinstall": "node src/scripts/copy-monaco.mjs || true"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.4.0",
    "@monaco-editor/react": "^4.6.0",
    "monaco-editor": "^0.45.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^31.7.6",
    "electron-vite": "^2.3.0",
    "fs-extra": "^11.2.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}